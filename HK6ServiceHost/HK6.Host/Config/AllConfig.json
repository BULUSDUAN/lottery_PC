{
  "RedisConfig": {
    "EnableRedis": true,
    "RedisHost": "10.0.3.6",
    "RedisPost": 6379,
    "RedisPassword": "redis123"

  },

  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "PageRegisterDefaultRole": "User",
  "Sports_SchemeJobSeconds": 20, //追号作业时间间隔，秒单位
  "SelfDomain": "http://localhost:729",
  "MobileDomain": "http://localhost:729",
  "ResSiteUrl": "http://10.0.3.6:888",

  "QueryUserFundDetailFromCache": false,
  //缓存配置文件，暂时没用到默认

  //必须配置
  "HostSettings": {
    "Ip": "127.0.0.1", //服务启动IP
    "WatchInterval": 30,
    "Port": 10108, //服务启动端口
    "Token": true,
    "RequestCacheEnabled": false
  },


  
  //Mongo 数据库配置
  "MongoSettings": {
    "CrawDataBaseIsMongo": false,
    "connectionString": "mongodb://10.0.3.6:27017",
    "SingleInstance": false,
    "IsStartLog": true, //是否启动记录日志
    "dbName": "CrawDB",
    //mogodb 采集表命名
    "TableNamesSettings": {
      "CreawPool": "NumLottery", //数字彩表名
      "CTZQMatch": "CTZQMatch" //传统足球表名
    }

  },
  //必须配置 服务发现配置
  "ConsulSettings": {
    "IpAddrs": "127.0.0.1:8500" //服务启动IP
  }


