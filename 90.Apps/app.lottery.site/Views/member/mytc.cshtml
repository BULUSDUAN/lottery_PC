@using app.lottery.site.Models
@using External.Core.Agnet
@using GameBiz.Core

@{    
    Layout = "../Shared/_LayoutFoud.cshtml";
    ViewBag.Title = "我的返点";
    ViewBag.keywords = "我的返点";
    var userRebate = ViewBag.InItNewUserRebate as OCAgentRebateInfoCollection;
    CurrentUserInfo currentUser = ViewBag.CurrentUser;
}

<div class="center mt10">
    @{Html.RenderAction("mycommenu", "Member");}
    @{ Html.RenderAction("toptipFoud", "Member"); }
    <div class="new_buy_right">
        <div class="betorder_top">
            <ul>
                @*<li class="betorder_topSelected"><a href="/member/myTc" checklg="/member/myTc">我的返点</a></li>*@
                @if (currentUser.LoginInfo.IsAgent)
                {
                    if (currentUser.LoginInfo.OCAgentType == OCAgentCategory.SportLotterySubAgent)
                    {
                        <li><a href="/member/subordinateuser" >返点用户</a></li>
                    }
                    else
                    {
                        <li><a href="/member/generalizeUser" >返点用户</a></li>
                    }
                    <li> <a href="/member/CommissionRecord">返点结算</a></li>
                    <li><a href="/member/StatisticsQuery">统计查询</a></li>
                    <li> <a href="/member/SchemeManage">方案管理</a></li>
                       
                }
            </ul>
        </div>
        <div class="betorder_con clearfix">
            <div class="scheme clearfix">
                <div class="scheme-show xzscheme">
                    <a href="/member/myTc"><i></i>我的返点</a>
                </div>
                <div class="scheme-show">
                    <a href="/member/detaileddescription">细则说明</a>
                </div>
            </div>
            @{
                //竞彩足球
                var jczq_brqspf = userRebate.FirstOrDefault(p => p.GameCode == "JCZQ" && p.GameType == "BRQSPF");
                var jczq_spf = userRebate.FirstOrDefault(p => p.GameCode == "JCZQ" && p.GameType == "SPF");
                var jczq_bf = userRebate.FirstOrDefault(p => p.GameCode == "JCZQ" && p.GameType == "BF");
                var jczq_zjq = userRebate.FirstOrDefault(p => p.GameCode == "JCZQ" && p.GameType == "ZJQ");
                var jczq_bqc = userRebate.FirstOrDefault(p => p.GameCode == "JCZQ" && p.GameType == "BQC");
                var jczq_hh = userRebate.FirstOrDefault(p => p.GameCode == "JCZQ" && p.GameType == "HH");
                //北京单场
                var bjdc_spf = userRebate.FirstOrDefault(p => p.GameCode == "BJDC" && p.GameType == "SPF");
                var bjdc_zjq = userRebate.FirstOrDefault(p => p.GameCode == "BJDC" && p.GameType == "ZJQ");
                var bjdc_sxds = userRebate.FirstOrDefault(p => p.GameCode == "BJDC" && p.GameType == "SXDS");
                var bjdc_bf = userRebate.FirstOrDefault(p => p.GameCode == "BJDC" && p.GameType == "BF");
                var bjdc_bqc = userRebate.FirstOrDefault(p => p.GameCode == "BJDC" && p.GameType == "BQC");
                //竞彩篮球
                var jclq_sf = userRebate.FirstOrDefault(p => p.GameCode == "JCLQ" && p.GameType == "SF");
                var jclq_rfsf = userRebate.FirstOrDefault(p => p.GameCode == "JCLQ" && p.GameType == "RFSF");
                var jclq_sfc = userRebate.FirstOrDefault(p => p.GameCode == "JCLQ" && p.GameType == "SFC");
                var jclq_dxf = userRebate.FirstOrDefault(p => p.GameCode == "JCLQ" && p.GameType == "DXF");
                var jclq_hh = userRebate.FirstOrDefault(p => p.GameCode == "JCLQ" && p.GameType == "HH");
                //传统足球
                var ctzq_t14c = userRebate.FirstOrDefault(p => p.GameCode == "CTZQ" && p.GameType == "T14C");
                var ctzq_tr9 = userRebate.FirstOrDefault(p => p.GameCode == "CTZQ" && p.GameType == "TR9");
                var ctzq_t6bqc = userRebate.FirstOrDefault(p => p.GameCode == "CTZQ" && p.GameType == "T6BQC");
                var ctzq_t4cjq = userRebate.FirstOrDefault(p => p.GameCode == "CTZQ" && p.GameType == "T4CJQ");
                //排列三大乐透
                var tycp_dlt = userRebate.FirstOrDefault(p => p.GameCode == "DLT" && p.GameType == "");
                var tycp_pl3 = userRebate.FirstOrDefault(p => p.GameCode == "PL3" && p.GameType == "");
                //福彩三D双色球
                var fc3d = userRebate.FirstOrDefault(p => p.GameCode == "FC3D" && p.GameType == "");
                var ssq = userRebate.FirstOrDefault(p => p.GameCode == "SSQ" && p.GameType == "");
                //高频彩  
                var gpc_cqssc = userRebate.FirstOrDefault(p => p.GameCode == "CQSSC" && p.GameType == "");
                var gpc_jx11x5 = userRebate.FirstOrDefault(p => p.GameCode == "JX11X5" && p.GameType == "");
            }
       
            <table border="0" cellpadding="0" cellspacing="0" class="betorder_table tab_c">
                <tr class="s-t-tr1">
                    <td width="210">
                        <i class="jczq"></i>
                        竞彩足球返点
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr class="show">
                    <td width="210" colspan="2">
                        <ul>
                            <li>让球胜平负：<label>@(jczq_spf!=null? jczq_spf.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>胜平负：<label>@(jczq_brqspf!=null?jczq_brqspf.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>比分：<label>@(jczq_bf!=null?jczq_bf.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>进球数：<label>@(jczq_zjq!=null?jczq_zjq.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>半全场：<label>@(jczq_bqc!=null?jczq_bqc.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>混合过关：<label>@(jczq_hh!=null?jczq_hh.Rebate.ToString("N2"):"0.00")</label></li>
                        </ul>
                    </td>
                </tr>
                <tr class="s-t-tr1">
                    <td width="210">
                        <i class="bjdc"></i>
                        北京单场
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr class="show">
                    <td width="210" colspan="2">
                        <ul>
                               <li>胜平负：<label>@(bjdc_spf != null ? bjdc_spf.Rebate.ToString("N2") : "0.00")</label></li>
                            <li>进球数：<label>@(bjdc_zjq!=null?bjdc_zjq.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>上下单双：<label>@(bjdc_sxds!=null?bjdc_sxds.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>比分：<label>@(bjdc_bf!=null?bjdc_bf.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>半全场：<label>@(bjdc_bqc!=null?bjdc_bqc.Rebate.ToString("N2"):"0.00")</label></li>
                        </ul>
                    </td>
                </tr>
                <tr class="s-t-tr1">
                    <td width="210">
                         <i class="jclq"></i>
                        竞彩篮球
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr  class="show">
                    <td width="210" colspan="2">
                        <ul>
                            <li>胜负：<label>@(jclq_sf != null ? jclq_sf.Rebate.ToString("N2") : "0.00")</label></li>
                            <li>让分胜负：<label>@(jclq_rfsf != null ? jclq_rfsf.Rebate.ToString("N2") : "0.00")</label></li>
                            <li>胜负差：<label>@(jclq_sfc != null ? jclq_sfc.Rebate.ToString("N2") : "0.00")</label></li>
                            <li>大小分：<label>@(jclq_dxf != null ? jclq_dxf.Rebate.ToString("N2") : "0.00")</label></li>
                            <li>混合过关：<label>@(jclq_hh != null ? jclq_hh.Rebate.ToString("N2") : "0.00")</label></li>
                        </ul>
                    </td>
                </tr>
                <tr class="s-t-tr1">
                    <td width="210">
                         <i class="ctzq"></i>
                        传统足球
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr  class="show">
                    <td width="210" colspan="2">
                        <ul>
                            <li>14场胜负：<label>@(ctzq_t14c != null ? ctzq_t14c.Rebate.ToString("N2") : "0.00")</label></li>
                            <li>胜负任9：<label>@(ctzq_tr9!=null?ctzq_tr9.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>6场半全：<label>@(ctzq_t6bqc!=null?ctzq_t6bqc.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>4场进球：<label>@(ctzq_t4cjq!=null?ctzq_t4cjq.Rebate.ToString("N2"):"0.00")</label></li>
                        </ul>
                    </td>
                </tr>
                <tr class="s-t-tr1">
                    <td width="210">
                        <i class="tycp"></i>
                        体育彩票
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr class="show">
                    <td width="210" colspan="2">
                        <ul>
                            <li>大乐透：<label>@(tycp_dlt!=null?tycp_dlt.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>排列三：<label>@(tycp_pl3!=null?tycp_pl3.Rebate.ToString("N2"):"0.00")</label></li>
                        </ul>
                    </td>
                </tr>
                <tr class="s-t-tr1">
                    <td width="210">
                        <i class="fc"></i>
                        福彩
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr class="show">
                    <td width="210" colspan="2">
                        <ul>
                            <li>双色球：<label>@(fc3d!=null?fc3d.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>福彩3D：<label>@(ssq!=null?ssq.Rebate.ToString("N2"):"0.00")</label></li>
                        </ul>
                    </td>
                </tr>
                <tr class="s-t-tr1">
                    <td width="210">
                        <i class="gpc"></i>
                        高频彩
                    </td>
                    <td class="unfoldorfold">
                        <a href="javascript:void(0)" class="toggleshow" >+ 展开</a>
                    </td>
                </tr>
                <tr  class="show">
                    <td width="210" colspan="2">
                        <ul>
                            <li>重庆时时彩：<label>@(gpc_cqssc!=null?gpc_cqssc.Rebate.ToString("N2"):"0.00")</label></li>
                            <li>江西11选5：<label>@(gpc_jx11x5 != null ? gpc_jx11x5.Rebate.ToString("N2") : "0.00")</label></li>
                        </ul>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <script type="text/javascript">
        $(".s-t-tr1").bind("click", function() {
            var tr = $(this).next(".show:first");
            if (tr.is(":visible")) {
                tr.hide(200);
                $(this).find(".toggleshow").text("+ 展开");
            } else {
                tr.show(300);
                $(this).find(".toggleshow").text("- 隐藏");
            }
        });      
    </script>
</div>
<div class="clearfix">
</div>
