<script type="text/javascript">
    $(".s-t-tr1").bind("click", function () {
        var tr = $(this).next(".show:first");

        if (tr.is(":visible")) {
            tr.hide(200);
            $(this).find(".toggleshow").text("+ 展开");
        } else {
            tr.show(300);
            $(this).find(".toggleshow").text("- 隐藏");
        }
    });      
</script>
