<script type="text/javascript">
    $(function () {
        $("input[name='txtxt']:text").attr("atomicselection", "false");
        var vs = $("input[name='txtxt']:text").val();
        if (vs == "") {
            $("input[name='txtxt']:text").val("0.00");
        }
        var error = 0;
        $("input[name='txtxt']:text").blur(function () {
            var val = $(this).attr("value");
            if (val == "") { $(this).attr("value", "0.00"); }
            if (val < 1) {
                //$(this).val(1);
            }
            else if (val > 100) {
                $(this).val(100);
            }

        }).keyup(function () {
            $(this).val($(this).val().replace(/[^0-9.]/g, ""));
        });

        $("#saveDealer").click(function () {

            if (!confirm("是否保存?")) {
                return false;
            }
            var _this = $(this);
            var dataArr = new Array();
            $("input[name='txtxt']:text").each(function () {
                var id = $(this).attr("id");
                var v = $(this).val();
                var rebateType = $(this).attr("rebateType");
                if (rebateType) {
                    dataArr.push(id + ":" + v + ":" + rebateType);
                }
                else {
                    dataArr.push(id + ":" + v + ":0");
                }
            });
            var dataStr = dataArr.join("|");
            var _id = $("#hdId").val();
            $.post("/member/savePZSNUR", { dataStr: dataStr, _id: _id }, function (data) {
                if (data.IsSuccess) {
                    alert(data.Message);
                    location.href = location.href;
                } else {
                    alert(data.Message);
                    return;
                }
            });
        });
    });    
</script>
