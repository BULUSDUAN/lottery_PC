@{
    var gameType = string.IsNullOrEmpty(Request["gameType"]) ? "" : Request["gameType"];
    if (ViewBag.GameCode == "newindex")
    {
        ViewBag.Title = "网上买彩票中大奖 | 网上彩票中奖查询 | 网上彩票开奖 | 选号 | 合买 | 代购 - " + SiteString.getHZSiteName(Request);
    }
    else
    {
        ViewBag.Title = SiteConvert.GameName(ViewBag.GameCode, gameType) + " - 体育彩票 | 福利彩票 | 中国体育彩票 - " + SiteString.getHZSiteName(Request);
    }

    ViewBag.keywords = "彩票开奖,网上彩票中大奖查询,福彩,体彩,足彩,竞彩,快开彩," + SiteString.getHZSiteName(Request);
    ViewBag.Description = "网上购买彩票，网上彩票中奖查询,网上彩票开奖查询，购买彩票中奖500W，购买彩票以及查询有没有中大奖请到" + SiteString.getHZSiteName(Request)+"查询。";
}
