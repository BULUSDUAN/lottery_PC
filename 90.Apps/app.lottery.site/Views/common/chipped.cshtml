                ViewBag.Title = SiteConvert.GameName(game, Request["type"]) + gameName + "合买-北单" + gameName + "合买-" + SiteString.getHZSiteName(Request) + "彩票网";
                ViewBag.Keywords = SiteConvert.GameName(game, Request["type"]) + gameName + "合买-北单" + gameName + "合买";
