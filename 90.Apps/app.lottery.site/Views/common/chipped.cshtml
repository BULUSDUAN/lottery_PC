        gameName = SiteConvert.GameName(game, PlayType);
    }
    if (!string.IsNullOrEmpty(game))
    {
        switch (game.ToLower())
        {
            case "jclq":
                gameName = gameName == "" ? "" : gameName.ToString().Substring(2);
                ViewBag.Title = SiteConvert.GameName(game, Request["type"]) + gameName + "合买-" + SiteString.getHZSiteName(Request) + "彩票网";
