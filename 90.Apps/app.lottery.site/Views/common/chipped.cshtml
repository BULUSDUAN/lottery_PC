                ViewBag.Keywords = "足彩" + gameName + "合买" + gameName + "合买";
                ViewBag.Description = SiteString.getHZSiteName(Request) + "彩票网合买频道为您提供专业的" + SiteConvert.GameName(game, Request["type"]) + gameName + "合买,更有最新的合买达人发布的" + SiteConvert.GameName(game, Request["type"]) + gameName + "合买方案," + SiteConvert.GameName(game, Request["type"]) + gameName + "合买就上" + SiteString.getHZSiteName(Request) + "彩票网.";
                betUrl = "/toto/" + gameType;
                break;
            case "bjdc":
