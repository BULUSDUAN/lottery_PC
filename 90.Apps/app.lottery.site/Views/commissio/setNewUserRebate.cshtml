<script type="text/javascript">
    var toggle = {
        bflick: function (obj) {
            var tr = $(obj).parents("tr").next(".show:first");
            if (tr.is(":visible")) {
                tr.hide();
                $(obj).text("+展开");

            } else {
                tr.show();
                $(obj).text("-折叠");
            }
        }
    }
    $(function () {
        $("input[name='txtxt']:text").attr("atomicselection", "false");
        var error = 0;
        $("input:text").blur(function () {
            var val = $(this).attr("value");
            var reg = /^[0-9]+(\.[0-9]+)?$/;
            if (val == "") { $(this).attr("value", "0.00"); }
            if (val != "" && !reg.test(val)) {
                error++;
                alert('只能输入数字！');
                $(this).css("color", "red");
                return false;
            } else {
                if (val < 0) {
                    error++;
                    alert('设置的返点必须是大于或者等于0且小于100的小数');
                    $(this).css("color", "red");
                    return false;
                }
                else if (val > 100) {
                    error++;
                    alert('设置的返点必须是大于或者等于0且小于100的小数');
                    $(this).css("color", "red");
                    return false;
                }
                else {
                    error = 0;
                    $(this).css("color", "");
                }
            }
        });

        $(".saveDealer").click(function () {
            if (error != 0) {
                alert("设置的返点必须是大于或者等于0且小于100的小数");
                return false;
            }
            if (!confirm("是否保存?")) {
                return false;
            }
            var _this = $(this);
            var dataArr = new Array();
            $("input[name='txtxt']:text").each(function () {
                var id = $(this).attr("gameList");
                var v = $(this).val();
                dataArr.push(id + ":" + v);
            })
            var dataStr = dataArr.join("|");
            $.post("/commissio/SaveSNUR", { dataStr: dataStr }, function (data) {
                if (data.IsSuccess) {
                    alert(data.Msg);
                    location.href = location.href;
                }
                else {
                    alert(data.Msg);
                    location.href = location.href;
                }
            })
        });
        //竞彩足球
        $("#txtjczq").blur(function () {

            applyData("txtjczq", $(this).val());
        });

        //北京单场
        $("#txtbjdc").blur(function () {
            applyData("txtbjdc", $(this).val());
        });

        //竞彩篮球
        $("#txtjclq").blur(function () {
            applyData("txtjclq", $(this).val());
        });

        //传统足球
        $("#txtctzq").blur(function () {
            applyData("txtctzq", $(this).val());
        });

        //体育彩票
        $("#txttycp").blur(function () {
            applyData("txttycp", $(this).val());
        });

        //福彩
        $("#txtfc").blur(function () {
            applyData("txtfc", $(this).val());
        });

        //高频彩
        $("#txtgpc").blur(function () {
            applyData("txtgpc", $(this).val());
        });

        function applyData(gameCode, value) {
            var setTxtVal = $("." + gameCode + "");
//            if (value > 0) {
//                setTxtVal.val(value);
            //            }
            setTxtVal.val(value);
        }
    });    
