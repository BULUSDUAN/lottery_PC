@using GameBiz.Core;
@{
    var award = ViewBag.Award as LotteryData.Core.GameWinNumber_Info;
    var issues = ViewBag.CurIssuse as Issuse_QueryInfo;
}
@GameHelper.LotteryHeaderView(issues, award,award == null||award.Id==0 ? "CQSSC" : award.GameCode)
@GameHelper.LotteryDescView(award == null||award.Id==0 ? "CQSSC" : award.GameCode)
<div class="lot-con-l">
    <h2 class="sctitle">
        <span class="der2"></span><b>时时彩奖级设置</b></h2>
    <div class="td_div2">
        <table width="100%" border="0" cellspacing="0" cellpadding="0" class="sg">
            <tbody>
                <tr>
                    <td>五星直选
                    </td>
                    <td class="td1">选5个号码，与开奖号码完全按位全部相符
                    </td>
                    <td>100000元
                    </td>
                </tr>
                <tr>
                    <td>五星通选
                    </td>
                    <td class="td1">选5个号码，与开奖号码前三位或后三位按位相符
                    </td>
                    <td>200元
                    </td>
                </tr>
                <tr>
                    <td>三星直选
                    </td>
                    <td class="td1">选3个号码，与开奖号码连续后三位按位相符
                    </td>
                    <td>1000元
                    </td>
                </tr>
                <tr>
                    <td>三星组选三
                    </td>
                    <td class="td1">选2个号码，与开奖号码连续后三位相符
                    </td>
                    <td>320元
                    </td>
                </tr>
                <tr>
                    <td>三星组选六
                    </td>
                    <td class="td1">选3个号码，与开奖号码连续后三位相符(顺序不限)
                    </td>
                    <td>160元
                    </td>
                </tr>
                <tr>
                    <td>二星直选
                    </td>
                    <td class="td1">选2个号码，与开奖号码连续后二位按位相符
                    </td>
                    <td>100元
                    </td>
                </tr>
                <tr>
                    <td>二星组选
                    </td>
                    <td class="td1">选2个号码，与开奖号码连续后二位相符
                    </td>
                    <td>50元
                    </td>
                </tr>
                <tr>
                    <td>一星
                    </td>
                    <td class="td1">选1个号码，与开奖号码个位相符
                    </td>
                    <td>10元
                    </td>
                </tr>
                <tr>
                    <td>大小单双
                    </td>
                    <td class="td1">与开奖号码后二位数字属性按位相符
                    </td>
                    <td>4元
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="lot-con-r">
    <h2 class="sctitle">
        <span class="der3"></span><b>时时彩资讯</b></h2>
    @{Html.RenderAction("LotteryConsulting", "buy", new { id = "CQSSC" });}
</div>
<div class="clearfix"></div>