    var pageSize = Request["pageSize"];
    if (pageSize != "" && pageSize != null)
    {
        ViewBag.Title = "大乐透奇偶走势图近" + pageSize + "期" + SiteString.getHZSiteName(Request);
        ViewBag.Keywords = "大乐透奇偶走势图近" + pageSize + "期";
        ViewBag.Description = SiteString.getHZSiteName(Request) + "大乐透奇偶走势图近" + pageSize + "期,查看更多大乐透奇偶走势图请登录" + SiteString.getHZSiteName(Request) + "彩票网.";
    }
