@{
    Layout = "~/Views/lotteryTrend/_Layout.cshtml";
    var pageSize = Request["pageSize"];
    if (pageSize != "" && pageSize != null)
    {
        ViewBag.Title = "重庆时时彩二星直选走势图近" + pageSize + "期" + SiteString.getHZSiteName(Request);
        ViewBag.Keywords = "重庆时时彩二星直选走势图近" + pageSize + "期";
        ViewBag.Description = SiteString.getHZSiteName(Request) + "重庆时时彩二星直选走势图近" + pageSize + "期,查看更多请登录" + SiteString.getHZSiteName(Request) + "彩票网.";
    }
    else
    {
        ViewBag.Title = "重庆时时彩二星直选走势图-" + SiteString.getHZSiteName(Request);
        ViewBag.Keywords = "重庆时时彩二星直选走势图";
        ViewBag.Description = SiteString.getHZSiteName(Request) + "彩票网提供最新的重庆时时彩二星直选走势图,其中包括十位和个位开奖号码的分布、大小比、奇偶比、质合比等的走势,让彩民朋友更直观的分析重庆时时彩二星直选走势图.";
    }
    var s2zxzs = ViewBag.DataList as List<LotteryData.Core.CQSSC_2X_ZXZS_Info>;
    var a = 0;
    var phasseOrder = ViewBag.PhasseOrder;
    string id = "s2zxzs_cqssc_" + phasseOrder;
    var size = string.IsNullOrEmpty(pageSize) ? ViewBag.PageSize : pageSize;
    string url = string.Format("/statichtml/lotteryTrend/CQSSC/s2zxzs{0}_up.html", size);
    if (phasseOrder == "up")
    {
        url = string.Format("/statichtml/lotteryTrend/CQSSC/s2zxzs{0}.html", size);
    }
}
