@{
    Layout = "~/Views/lotteryTrend/_Layout.cshtml";
    var pageSize = Request["pageSize"];
    if (pageSize != "" && pageSize != null)
    {
        ViewBag.Title = "大乐透大小走势图近" + pageSize + "期" + SiteString.getHZSiteName(Request);
        ViewBag.Keywords = "大乐透大小走势图近" + pageSize + "期";
        ViewBag.Description = SiteString.getHZSiteName(Request) + "大乐透大小走势图近" + pageSize + "期,查看更多大乐透大小走势图请登录" + SiteString.getHZSiteName(Request) + "彩票网.";
    }
    else
    {
        ViewBag.Title = "大乐透大小走势图-大乐透走势图表-" + SiteString.getHZSiteName(Request);
        ViewBag.Keywords = "大乐透大小走势图,大乐透走势图表";
        ViewBag.Description ="查看大乐透大小走势图就上" + SiteString.getHZSiteName(Request) + "彩票网," + SiteString.getHZSiteName(Request) + "彩票网竭诚为您提供最新的大乐透走势图表,包括各位号码的大小号分布情况,更多的大乐透大小走势图请登录" + SiteString.getHZSiteName(Request) + "彩票网进行查看.";
